---
import { Icon } from "astro-icon/components";
const { name, link } = Astro.props;
---

<div
  class="flex justify-center items-center px-1 md:p-2 md:transition-transform duration-340 md:hover:scale-125"
>
  <Icon name={name} class="h-7 md:h-8 w-7 md:w-8 text-accentd1 md:hover:text-primary" />
</div>
